-- 最终版全行汇总票据转帖现统计表_展示  TBIEE_ZTXYWSY_SHOW

SELECT
  TD.THE_YEAR AS THE_YEAR,
  TD.THE_MONTH AS THE_MONTH,
  TZ.PROD_TYPE AS PROD_TYPE,
  TZ.CALC_TYPE AS CALC_TYPE,
  TZ.TOTAL_COUNT AS TOTAL_COUNT,
  TZ.SUN_MONEY AS SUN_MONEY,
  TZ.INCOME AS INCOME

  FROM TBIEE_ZTXYWSY TZ,TBIEE_WORKDATE TW LEFT JOIN TBIEE_DATETIME TD ON TD.THE_DATE=TW.WORKDAY 
  WHERE TZ.BUILD_DAY = TW.WORKDAY 
